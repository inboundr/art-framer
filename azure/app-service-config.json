{
  "name": "cooart-studio",
  "location": "East US",
  "kind": "app",
  "properties": {
    "serverFarmId": "/subscriptions/{subscription-id}/resourceGroups/cooart-rg/providers/Microsoft.Web/serverfarms/cooart-plan",
    "siteConfig": {
      "appSettings": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_URL",
          "value": "https://irugsjzjqdxulliobuwt.supabase.co"
        },
        {
          "name": "NEXT_PUBLIC_SUPABASE_ANON_KEY",
          "value": "your-anon-key-here"
        },
        {
          "name": "SUPABASE_SERVICE_ROLE_KEY",
          "value": "your-service-role-key-here"
        },
        {
          "name": "STRIPE_SECRET_KEY",
          "value": "your-stripe-secret-key-here"
        },
        {
          "name": "STRIPE_PUBLISHABLE_KEY",
          "value": "your-stripe-publishable-key-here"
        },
        {
          "name": "STRIPE_WEBHOOK_SECRET",
          "value": "your-stripe-webhook-secret-here"
        },
        {
          "name": "PRODIGI_API_KEY",
          "value": "11fc6ec8-855e-4b32-a36a-3a80db5d5ea6"
        },
        {
          "name": "PRODIGI_ENVIRONMENT",
          "value": "sandbox"
        },
        {
          "name": "NEXT_PUBLIC_SHOW_NOTIFICATION_BAR",
          "value": "false"
        }
      ],
      "linuxFxVersion": "NODE|18-lts",
      "use32BitWorkerProcess": false,
      "webSocketsEnabled": true,
      "alwaysOn": true,
      "httpLoggingEnabled": true,
      "logsDirectorySizeLimit": 35,
      "detailedErrorLoggingEnabled": true,
      "publishingUsername": "$cooart-studio",
      "scmType": "GitHub",
      "ftpsState": "Disabled",
      "managedPipelineMode": "Integrated",
      "virtualApplications": [
        {
          "virtualPath": "/",
          "physicalPath": "site\\wwwroot",
          "preloadEnabled": true
        }
      ],
      "loadBalancing": "LeastRequests",
      "experiments": {
        "rampUpRules": []
      },
      "autoHealEnabled": false,
      "vnetRouteAllEnabled": false,
      "vnetPrivatePortsCount": 0,
      "publicNetworkAccess": "Enabled",
      "keyVaultReferenceIdentity": "SystemAssigned",
      "ipSecurityRestrictions": [],
      "scmIpSecurityRestrictions": [],
      "scmIpSecurityRestrictionsUseMain": false,
      "http20Enabled": true,
      "minTlsVersion": "1.2",
      "scmMinTlsVersion": "1.2",
      "ftpsState": "Disabled",
      "preWarmedInstanceCount": 0,
      "functionAppScaleLimit": 0,
      "healthCheckPath": "/api/health",
      "healthCheckPath": "/api/health"
    },
    "httpsOnly": true,
    "redundancyMode": "None",
    "storageAccountRequired": false,
    "keyVaultReferenceIdentity": "SystemAssigned"
  }
